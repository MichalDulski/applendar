{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt"
        }
      }
    ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": {
          "maximumDestructuringDepth": 4
        }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": {
          "maximumStringLength": 100
        }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": {
          "maximumCollectionCount": 10
        }
      }
    ],
    "Properties": {
      "Application": "Applendar.API"
    }
  },
  "ConnectionStrings": {
    "SQL_CONNECTION_STRING": "Host=postgresql_server;Database=postgres;Username=postgres;Password=LocalUserPasswd"
  },
  "Authentication": {
    "Auth0": {
      "Domain": "",
      "Audience": "",
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "Migrate": {
    "MigrateOnStartup": "true"
  }
}